<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="security.authentication.factory.sso"
          class="AsaAyers\CrowdBundle\DependencyInjection\Security\Factory\SsoFactory" public="false">
            <tag name="security.listener.factory" />
        </service>
        <service id="security.authentication.factory.crowd"
          class="AsaAyers\CrowdBundle\DependencyInjection\Security\Factory\CrowdFactory" public="false">
            <tag name="security.listener.factory" />
        </service>
    </services>
</container>

